{
    "name": "frontend",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@braintree/sanitize-url": "^6.0.4",
        "@reduxjs/toolkit": "^1.9.5",
        "@testing-library/jest-dom": "^5.17.0",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^13.5.0",
        "@types/jest": "^27.5.2",
        "@types/node": "^16.18.43",
        "@types/react": "^18.2.21",
        "@types/react-dom": "^18.2.7",
        "concurrently": "^8.2.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-helmet-async": "^1.3.0",
        "react-intl": "^6.4.4",
        "react-redux": "^8.1.2",
        "react-router-dom": "^6.15.0",
        "react-scripts": "5.0.1",
        "react-slick": "^0.29.0",
        "redux-actions": "^3.0.0",
        "redux-mock-store": "^1.5.4",
        "redux-saga": "^1.2.3",
        "redux-saga-routines": "^3.2.3",
        "slick-carousel": "^1.8.1",
        "storybook-addon-react-router-v6": "^2.0.7",
        "styled-components": "^6.0.7",
        "styled-reset": "^4.5.1",
        "typescript": "^4.9.5",
        "web-vitals": "^2.1.4"
    },
    "scripts": {
        "start-default": "react-scripts start",
        "start": "concurrently -k -p \"[{name}]\" -n \"Frontend\" -c \"bgCyan.bold\" \"npm run start-default\"",
        "build": "react-scripts build",
        "test": "react-scripts test --watchAll=false",
        "test:update": "npm run test -- -u",
        "eject": "react-scripts eject",
        "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
        "lint": "eslint --ext js,jsx,ts,tsx src --max-warnings=0",
        "lint:fix": "eslint --fix \"**/*.ts\" \"**/*.tsx\" --max-warnings=0",
        "storybook": "storybook dev -p 6006",
        "build-storybook": "storybook build"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@storybook/addon-essentials": "^7.3.2",
        "@storybook/addon-interactions": "^7.3.2",
        "@storybook/addon-links": "^7.3.2",
        "@storybook/addon-onboarding": "^1.0.8",
        "@storybook/blocks": "^7.3.2",
        "@storybook/preset-create-react-app": "^7.3.2",
        "@storybook/react": "^7.3.2",
        "@storybook/react-webpack5": "^7.3.2",
        "@storybook/testing-library": "^0.2.0",
        "@types/react-slick": "^0.23.10",
        "@types/redux-mock-store": "^1.0.3",
        "@types/redux-saga-routines": "^3.1.4",
        "babel-plugin-named-exports-order": "^0.0.2",
        "eslint": "^8.47.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-typescript": "^17.1.0",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-import": "^2.28.1",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-prettier": "^5.0.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "prettier": "^3.0.2",
        "prop-types": "^15.8.1",
        "storybook": "^7.3.2",
        "webpack": "^5.88.2"
    },
    "jest": {
        "transformIgnorePatterns": [
            "/node_modules/(?!axios)/"
        ]
    }
}
